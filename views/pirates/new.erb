<h1>Welcome to the Nested Forms Lab!</h1>
<form action="/pirates" method="post">
  <label id="pirate_name">Name: <input id="pirate_name" type="text" name="pirate[name]"></label>
  <label id="pirate_weight">Weight: <input id="pirate_weight" type="text" name="pirate[weight]"></label>
  <label id="pirate_height">Height: <input id="pirate_height" type="text" name="pirate[height]"></label>

  <label id="ship_name_1">Ship Name: <input id="ship_name_1" type="text" name="pirate[ships][][name]"></label>
  <label id="ship_type_1">Ship Type: <input id="ship_type_1" type="text" name="pirate[ships][][type]"></label>
  <label id="ship_booty_1">Ship Booty: <input id="ship_booty_1" type="text" name="pirate[ships][][booty]"></label>

  <label id="ship_name_2">Ship Name: <input id="ship_name_2" type="text" name="pirate[ships][][name]"></label>
  <label id="ship_type_2">Ship Type: <input id="ship_type_2" type="text" name="pirate[ships][][type]"></label>
  <label id="ship_booty_2">Ship Booty: <input id="ship_booty_2" type="text" name="pirate[ships][][booty]"></label>
  <button type="submit" id="Submit">Submit</button>
</form>
